package com.bo.elements{	import flash.events.MouseEvent;	import flash.display.MovieClip		import flash.display.Sprite;	public class BaseSymbol extends MovieClip{		var w:Number;		var h:Number;		public function BaseSymbol() {			// constructor code			buttonMode=true;			gotoAndStop(1);			addEventListener(MouseEvent.ROLL_OVER,onOver);			addEventListener(MouseEvent.ROLL_OUT,onOut);		}		function onOver(evt:MouseEvent):void{			gotoAndStop(2);		}		function onOut(evt:MouseEvent):void{			gotoAndStop(1);		}	}	}